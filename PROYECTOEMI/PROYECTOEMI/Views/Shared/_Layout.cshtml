    <!--For Plugins external css-->
    <link href="~/lib/Template/css/animate/animate.css" rel="stylesheet" />

    <link href="~/lib/Template/css/plugins.css" rel="stylesheet" />
    <!--Theme custom css -->
    <link href="~/lib/Template/css/style.css" rel="stylesheet" />
    <!--Theme Responsive css-->
    <link href="~/lib/Template/css/responsive.css" rel="stylesheet" />
    <script src="~/lib/Template/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>

</head>
<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <div class='preloader'><div class='loaded'>&nbsp;</div></div>
    <header id="home" class="navbar-fixed-top">
        <div class="header_top_menu clearfix">
            <div class="container">
                <div class="row">
                    <div class="col-md-5 col-md-offset-3 col-sm-12 text-right">
                        <div class="call_us_text">
                            <a href=""><i class="fa fa-clock-o"></i> Ordene Comida 24/7</a>
                            <a href=""><i class="fa fa-phone"></i>315 757 08 71</a>
                        </div>
                    </div>

                    <div class="col-md-4 col-sm-12">
                        <div class="head_top_social text-right">
                            <a href=""><i class="fa fa-facebook"></i></a>
                            <a href=""><i class="fa fa-twitter"></i></a>
                            <a href=""><i class="fa fa-youtube"></i></a>
                            <a href=""><i class="fa fa-phone"></i></a>
                            <a href=""><i class="fa fa-camera"></i></a>
                        </div>
                    </div>

                </div>
            </div>
        </div>

        <!-- End navbar-collapse-->

        <div class="main_menu_bg">
            <div class="container">
                <div class="row">
                    <nav class="navbar navbar-default">
                        <div class="container-fluid">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <a class="navbar-brand our_logo" href="#"><img src="~/lib/Template/images/logo.png" width="200" /></a>
                                
                            </div>

                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                                <ul class="nav navbar-nav navbar-right">
                                    <li><a href="/Home/Index">Inicio</a></li>
                                    <li><a href="/Home/Index#abouts">Nosotros</a></li>
                                    <li><a href="/Home/Index#features">Especialidades</a></li>
                                    <li><a href="/Home/Index#portfolio">Menú</a></li>
                                    <li><a href="/Home/Index#ourPakeg">Bebidas</a></li>
                                    <li><a href="/Home/Index#mobaileapps">Páginas</a></li>
                                    @using Microsoft.AspNetCore.Identity
                                    @inject SignInManager<IdentityUser> SignInManager
                                    @inject UserManager<IdentityUser> UserManager


                                    @if (SignInManager.IsSignedIn(User))
                                    {
                                        <li>
                                            <a asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Hola @User.Identity.Name!</a>
                                        </li>
                                        <li>
                                            <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                                                <button type="submit">CERRAR SESIÓN</button>
                                            </form>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a asp-area="Identity" asp-page="/Account/Login">Ingresar</a>
                                        </li>
                                        <li>
                                            <a  class="booking" asp-area="Identity" asp-page="/Account/Register">Registrarse</a>
                                        </li>

                                    }
                                </ul>
                            </div><!-- /.navbar-collapse -->
                        </div><!-- /.container-fluid -->
                    </nav>
                </div>
            </div>
        </div>
    </header> <!-- End Header Section -->

    
    <section class="contenido">
        @RenderBody()
    </section>


    <section id="footer_widget" class="footer_widget">
        <div class="container">
            <div class="row">
                <div class="footer_widget_content text-center">
                    <div class="col-md-4">
                        <div class="single_widget wow fadeIn" data-wow-duration="2s">
                            <h3>Contactanos</h3>

                            <div class="single_widget_info">
                                <p>
                                    <span>Propietaria Lina Ocampo</span>
                                    <span class="phone_email">WhatsApp: 315 767 08 71</span>
                                    <span>Email: ComidasEmi@gmail.com</span>
                                </p>
                            </div>

                            <div class="footer_socail_icon">
                                <a href=""><i class="fa fa-facebook"></i></a>
                                <a href=""><i class="fa fa-twitter"></i></a>
                                <a href=""><i class="fa fa-youtube"></i></a>
                                <a href=""><i class="fa fa-phone"></i></a>
                                <a href=""><i class="fa fa-camera"></i></a>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-4">
                        <div class="single_widget wow fadeIn" data-wow-duration="4s">
                            <h3>Horario de atencion</h3>

                            <div class="single_widget_info">
                                <p>
                                    <span class="date_day">Lunes a jueves</span>
                                    <span>1:00pm a 12:00m</span>

                                    <span class="date_day">Viernes & Sabados</span>
                                    <span>1:00pm a 3:00am</span>

                                    <span class="date_day">Domingos y Festivos</span>
                                    <span>1:00pm a 1:30am</span>
                                </p>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-4">
                        <div class="single_widget wow fadeIn" data-wow-duration="5s">
                            <h3>Envianos un mensaje</h3>

                            <div class="single_widget_form text-left">
                                <form action="#" id="formid">
                                    <div class="form-group">
                                        <input type="text" class="form-control" name="name" placeholder="Nombre" required="">
                                    </div>

                                    <div class="form-group">
                                        <input type="email" class="form-control" name="email" placeholder="Email" required="">
                                    </div>

                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Tema">
                                    </div> <!-- end of form-group -->

                                    <div class="form-group">
                                        <textarea class="form-control" name="message" rows="3" placeholder="Mensaje"></textarea>
                                    </div>

                                    <input type="submit" value="Enviar" class="btn btn-primary">
                                </form>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>



    <!--Footer-->
    <center>
        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - COMIDASRÁPIDASEMI - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>
    </center>


    <div class="scrollup">
        <a href="#"><i class="fa fa-chevron-up"></i></a>
    </div>


    <script src="~/lib/Template/js/vendor/jquery-1.11.2.min.js"></script>
    <script src="~/lib/Template/js/vendor/bootstrap.min.js"></script>

    <script src="~/lib/Template/js/jquery-easing/jquery.easing.1.3.js"></script>
    <script src="~/lib/Template/js/wow/wow.min.js"></script>
    <script src="~/lib/Template/js/plugins.js"></script>
    <script src="~/lib/Template/js/main.js"></script>
    @RenderSection("Scripts", required: false)

</body>
</html>